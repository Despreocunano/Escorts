    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "astro",
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "env": {
      "PUBLIC_SUPABASE_URL": "@public_supabase_url",
      "PUBLIC_SUPABASE_ANON_KEY": "@public_supabase_anon_key"
    }
  }